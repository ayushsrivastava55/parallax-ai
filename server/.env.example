### Eyebalz Gateway Standalone Server ###

PORT=3000

# MiniMax M2.5 (research model — OpenAI-compatible chat completions)
MINIMAX_API_KEY=
OPENAI_BASE_URL=https://api.minimax.io/v1
OPENAI_LARGE_MODEL=MiniMax-M2.5
OPENAI_SMALL_MODEL=MiniMax-M2.5

# MiniMax Search MCP server (web research)
SERPER_API_KEY=          # https://serper.dev (2500 free/month)
JINA_API_KEY=            # https://jina.ai (free tier)

# Auth (set ALLOW_UNSIGNED=true for dev)
EYEBALZ_GATEWAY_ALLOW_UNSIGNED=true
EYEBALZ_KEYS_JSON={}
EYEBALZ_GATEWAY_SIGNING_SECRET=dev-secret
EYEBALZ_GATEWAY_REPLAY_WINDOW_SEC=60
EYEBALZ_GATEWAY_QUOTE_TTL_SEC=90

# Connectors
OPINION_API_KEY=
OPINION_ENABLED=false
OPINION_EXECUTION_ENABLED=false
BNB_PRIVATE_KEY=
BNB_PUBLIC_KEY=

# Policy
EYEBALZ_GATEWAY_ALLOWED_PLATFORMS=predictfun,opinion
EYEBALZ_GATEWAY_MAX_ORDER_USD=1000
EYEBALZ_GATEWAY_MAX_SLIPPAGE_BPS=300
EYEBALZ_GATEWAY_KILL_SWITCH=false

# Yield
YIELD_MIN_IDLE_USD=500
YIELD_RECALL_BUFFER_USD=200
YIELD_MAX_UTILIZATION_PCT=80

# Position ledger
EYEBALZ_POSITION_LEDGER_PATH=.eyebalz/trade-fills.jsonl

# ERC-8004 (optional)
ERC8004_ENABLED=false
ERC8004_IDENTITY_REGISTRY=
ERC8004_REPUTATION_REGISTRY=
ERC8004_VALIDATION_REGISTRY=
FLASH_AGENT_CONTRACT=
ERC8004_AGENT_ID=
BSC_TESTNET_RPC=https://data-seed-prebsc-1-s1.binance.org:8545

