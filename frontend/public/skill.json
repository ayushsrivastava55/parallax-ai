{
  "name": "eyebalz-gateway",
  "version": "2.0.0",
  "description": "Autonomous prediction market trading skill for OpenClaw bots on BNB Chain",
  "homepage": "https://eyebalz.xyz",
  "api_base": "https://eyebalz.xyz/api/v1",
  "category": "trading",
  "autonomous": true,
  "platforms": ["predictfun", "probable"],
  "files": {
    "skill": "/skill.md",
    "market_intel": "/skill-market-intel.md",
    "trading": "/skill-trading.md",
    "portfolio": "/skill-portfolio.md",
    "identity": "/skill-identity.md",
    "heartbeat": "/heartbeat.md",
    "messaging": "/messaging.md",
    "rules": "/rules.md",
    "strategy_playbook": "/strategy-playbook.md",
    "strategy_delta_neutral": "/strategy-delta-neutral.md",
    "strategy_thesis": "/strategy-thesis.md",
    "strategy_yield": "/strategy-yield.md"
  },
  "routing": {
    "markets_or_analysis": "/skill-market-intel.md",
    "execution": "/skill-trading.md",
    "positions_or_yield": "/skill-portfolio.md",
    "identity_or_reputation": "/skill-identity.md",
    "strategy": "/strategy-playbook.md",
    "heartbeat": "/heartbeat.md"
  },
  "auth": {
    "scheme": "hmac-sha256",
    "headers": ["X-Eyebalz-Agent-Id", "X-Eyebalz-Key-Id", "X-Eyebalz-Timestamp", "X-Eyebalz-Nonce", "X-Eyebalz-Signature"]
  },
  "execution": {
    "requires_confirmation_token": true,
    "requires_idempotency_key": true
  }
}
